{
	"name": "creatium",
	"version": "0.2.0",
	"description": "Build your create-bins quickly and easily",
	"keywords": [
		"bin",
		"cli",
		"create",
		"create-bins",
		"library",
		"node",
		"pigeonposse",
		"pp"
	],
	"homepage": "https://creatium.pigeonposse.com/guide/core",
	"bugs": {
		"url": "https://github.com/pigeonposse/creatium/issues",
		"email": "dev@pigeonposse.com"
	},
	"repository": {
		"type": "https",
		"url": "https://github.com/pigeonposse/creatium/",
		"directory": "packages/core"
	},
	"funding": {
		"type": "individual",
		"url": "https://pigeonposse.com/?popup=donate"
	},
	"license": "GPL-3.0",
	"author": {
		"name": "Angelo",
		"email": "angelo@pigeonposse.com",
		"url": "https://github.com/angelespejo"
	},
	"type": "module",
	"exports": {
		".": {
			"import": {
				"types": "./dist/index.d.mts",
				"default": "./dist/index.mjs"
			},
			"require": {
				"types": "./dist/index.d.cts",
				"default": "./dist/index.cjs"
			}
		}
	},
	"main": "dist/index.mjs",
	"module": "dist/index.mjs",
	"types": "dist/index.d.ts",
	"files": [
		"dist"
	],
	"scripts": {
		"build": "unbuild",
		"dev": "pnpx tsx examples/simple/bin.ts",
		"example": "run(){ tsx examples/\"${@:1}\"/bin.ts; }; run $@",
		"size": "pnpm dlx @sizium/cli -i ./ --res info",
		"test": "vitest run -r src --passWithNoTests"
	},
	"dependencies": {
		"@clippium/preset-colored": "1.0.0",
		"@creatium-js/utils": "workspace:*",
		"clippium": "0.1.0",
		"tiny-updater": "3.5.3"
	},
	"devDependencies": {
		"@creatium-js/repo-config": "workspace:*"
	},
	"publishConfig": {
		"access": "public",
		"registry": "https://registry.npmjs.org/"
	}
}
